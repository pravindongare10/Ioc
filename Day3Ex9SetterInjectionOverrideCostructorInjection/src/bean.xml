<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
 <!-- services -->
 
 <bean id="s1" class="com.model.Student">
 
 	<constructor-arg name="id" value="1"></constructor-arg>
 	<constructor-arg name="name" value="Pravin"></constructor-arg>
 	<constructor-arg name="adr" value="Pangari"></constructor-arg>
 
	<property name="id" value="2"></property>
 	<property name="name" value="Rahul"></property>
 	<property name="adr" value="Mumbai"></property>
 </bean>
 
</beans>
